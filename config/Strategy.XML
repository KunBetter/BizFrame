<Beans>
    <bean id="bizService" class="com.BizService">
        <property name="bizDispatcher" ref="bizDispatcher"/>
    </bean>

    <bean id="bizDispatcher" class="com.GenericBizDispatcher">
        <constructor-arg type="java.util.List">
            <list>
                <ref bean="defaultStrategyGroup"/>
            </list>
        </constructor-arg>
    </bean>

    <bean id="defaultStrategyGroup" class="com.StrategyGroup">
        <constructor-arg value="default"/>
        <constructor-arg type="java.util.Map">
            <map>
                <entry key="default" value-ref="defaultStrategy"/>
            </map>
        </constructor-arg>
        <constructor-arg ref="defaultStrategy"/>
    </bean>

    <bean id="logger" class="com.BizLogger"/>
    <bean id="errorHandler" class="com.ErrorHandler"/>

    <bean id="defaultStrategy" class="com.BizStrategy">
        <constructor-arg>
            <list>
                # Go IOC ï¼Ÿ
                <ref bean="handler1"/>
                <ref bean="handler2"/>
            </list>
        </constructor-arg>
        <constructor-arg ref="logger"/>
        <property name="errorHandler" ref="errorHandler"/>
    </bean>
</Beans>